{
  "name": "systemjs-seed",
  "version": "1.0.0",
  "description": "seed project for systemjs and angular",
  "devDependencies": {
    "gulp": "^3.8.11",
    "gulp-helpers": "^1.1.12",
    "jspm": "^0.14.0",
    "lodash": "^3.3.1",
    "run-sequence": "^1.0.2"
  },
  "dependencies": {},
  "jspm": {
    "directories": {
